{
  "name": "@remote-kakao/core",
  "type": "module",
  "version": "1.0.0",
  "description": "A Node.js module that helps you create KakaoTalk bots.",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./*": {
      "require": "./dist/cjs/*/index.js",
      "import": "./dist/esm/*/index.js"
    }
  },
  "scripts": {
    "build": "$npm_execpath build:esm && $npm_execpath build:cjs",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json"
  },
  "repository": "https://github.com/remote-kakao/core.git",
  "author": "thoratica <me@tica.fun>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.19",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  }
}
